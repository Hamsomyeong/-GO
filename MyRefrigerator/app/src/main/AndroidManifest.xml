    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.MyRefrigerator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
      
        <activity android:name="com.example.MyRefrigerator.MemoActivity" />
        <activity android:name="com.example.MyRefrigerator.CategoryActivity" />
        <activity android:name="com.example.MyRefrigerator.ActionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.MyRefrigerator.AddActivity" />
        <activity android:name="com.example.MyRefrigerator.TestActivity" />
        <activity android:name="com.example.MyRefrigerator.FoodinfoActivity" />
        <activity android:name="com.example.MyRefrigerator.in_refrigerator" />
        <activity android:name="com.example.MyRefrigerator.RecipeAdd" />
        <activity android:name="com.example.MyRefrigerator.IngredientActivity" />
        <activity android:name="com.example.MyRefrigerator.SearchRecipe" />
        <activity android:name="com.example.MyRefrigerator.Search" />
        <activity android:name="com.example.MyRefrigerator.MainActivity2" />
        <activity android:name="com.example.MyRefrigerator.MainActivity" />
    </application>

</manifest>